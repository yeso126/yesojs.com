{
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
      },
      "rules": {
        "semi": 1,
        "quotes": [1, "single"]
      }
}
